{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "baseUrl": "./src",
  "typeRoots": [
    "./src/types",
    "./node-modules/@types",
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "paths": {
      "@component/*": [
        "src/components/*"
      ],
      "@chart/*": [
        "src/components/chart/*"
      ],
      "@mdxComponent/*": [
        "src/components/mdx/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@layout/*": [
        "src/layouts/*"
      ],
      "@type/*": [
        "src/types/*"
      ],
    },
    "types": [
      "astro/client",
    ]
  }
}