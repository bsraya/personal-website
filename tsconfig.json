{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "baseUrl": "./src",
  "typeRoots": [
    "./src/types",
    "./node-modules/@types",
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": [
      "astro/client",
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/client",
      "vite-plugin-pwa/solid",
      "vite-plugin-pwa/info",
    ]
  }
}